{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/instructor/(.*)",
      "dest": "/instructor-app/$1"
    },
    {
      "src": "/student/(.*)",
      "dest": "/student-app/$1"
    },
    {
      "src": "/instructor",
      "dest": "/instructor-app/index.html"
    },
    {
      "src": "/student",
      "dest": "/student-app/index.html"
    },
    {
      "src": "/",
      "dest": "/server/index.js"
    }
  ]
}
